{
  "name": "modernize-ng1",
  "version": "1.0.0",
  "description": "a sample AngularJS 1.x app for getting set up with parcel bundler...\n\nnow adapted to use my app of Ice and Fire front-end, for a non-trivial example",
  "main": "src/main.js",
  "scripts": {
    "clean": "rimraf dist",
    "dev": "parcel ./src/index.html",
    "format": "prettier --write \"src/**/*.{js,json}\"",
    "build": "parcel build ./src/index.html --public-url .",
    "build:sw": "rimraf dist/sw.js && workbox generateSW config/workbox-config.js",
    "build:prod": "run-s test build:quiet build:sw",
    "build:quiet": "parcel build ./src/index.html --public-url . --log-level 2",
    "test": "jest --config config/tests/jest.conf.js",
    "test:watch": "npm test -- --watch"
  },
  "keywords": [],
  "author": "Eric McCormick (https://edm00se.codes/)",
  "license": "MIT",
  "dependencies": {
    "@uirouter/angularjs": "1.0.24",
    "angular": "1.7.9",
    "angular-aria": "^1.7.9",
    "angular-messages": "^1.7.9",
    "angular-sanitize": "^1.7.9",
    "bootstrap": "3.4.1",
    "bootswatch": "3.4.1",
    "font-awesome": "4.7.0",
    "jquery": "3.4.1",
    "ng-tags-input": "3.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.7.7",
    "@babel/polyfill": "^7.7.0",
    "@babel/preset-env": "^7.7.7",
    "angular-mocks": "^1.7.9",
    "babel-jest": "24.9.0",
    "babel-plugin-angularjs-annotate": "0.10.0",
    "browserslist": "^4.8.3",
    "jest": "24.9.0",
    "npm-run-all": "4.1.5",
    "parcel-bundler": "1.12.4",
    "parcel-plugin-goodie-bag": "2.0.0",
    "prettier": "^1.19.1",
    "rimraf": "3.0.0",
    "sass": "^1.23.7",
    "workbox-cli": "4.3.1"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "ie 11",
    "not ie <= 10"
  ]
}
