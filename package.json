{
  "name": "modernize-ng1",
  "version": "1.0.0",
  "description": "a sample Angular 1.x app for getting set up with parcel bundler...\n\nnow adapted to use my app of Ice and Fire front-end, for a non-trivial example",
  "main": "src/main.js",
  "scripts": {
    "clean": "rimraf dist",
    "dev": "parcel ./src/index.html",
    "format": "prettier --write \"src/**/*.{js,json}\"",
    "build": "parcel build ./src/index.html --public-url .",
    "build:sw": "rimraf dist/sw.js && workbox generateSW config/workbox-config.js",
    "build:travis": "parcel build ./src/index.html --public-url . --log-level 2",
    "update-caniuse": "npm update caniuse-lite browserslist",
    "deploy": "npm run build && gh-pages -d dist",
    "test": "jest --config config/tests/jest.conf.js",
    "test:watch": "npm test -- --watch"
  },
  "keywords": [],
  "author": "Eric McCormick (https://edm00se.codes/)",
  "license": "MIT",
  "dependencies": {
    "@uirouter/angularjs": "1.0.20",
    "angular": "1.7.7",
    "angular-aria": "1.7.7",
    "angular-messages": "1.7.7",
    "angular-sanitize": "1.7.7",
    "bootstrap": "3.4.1",
    "bootswatch": "3.4.0",
    "font-awesome": "4.7.0",
    "jquery": "3.4.1",
    "ng-tags-input": "3.2.0"
  },
  "devDependencies": {
    "@babel/core": "7.6.4",
    "@babel/polyfill": "7.6.0",
    "@babel/preset-env": "7.6.3",
    "angular-mocks": "1.7.5",
    "babel-jest": "24.1.0",
    "babel-plugin-angularjs-annotate": "0.10.0",
    "browserslist": "4.7.2",
    "jest": "24.1.0",
    "npm-run-all": "4.1.5",
    "parcel-bundler": "1.12.4",
    "parcel-plugin-goodie-bag": "1.0.1",
    "prettier": "1.15.3",
    "rimraf": "2.6.2",
    "sass": "1.17.0",
    "workbox-cli": "4.3.1"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "ie 11",
    "not ie <= 10"
  ]
}
