{
  "name": "modernize-ng1",
  "version": "1.0.0",
  "description":
    "a sample Angular 1.x app for getting set up with parcel bundler...\n\nnow adapted to use my app of Ice and Fire front-end, for a non-trivial example",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf dist",
    "dev": "parcel index.html --open --no-cache",
    "prebuild": "npm run clean && echo 'should use ng-annotate here...'",
    "build": "parcel build index.html --public-url . --no-cache",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [],
  "author": "Eric McCormick (https://ericmccormick.io/)",
  "license": "MIT",
  "dependencies": {
    "@uirouter/angularjs": "1.0.15",
    "angular": "1.6.9",
    "angular-messages": "1.6.9",
    "angular-sanitize": "1.6.9",
    "autoprefixer": "8.2.0",
    "babel-cli": "6.26.0",
    "babel-plugin-ng-annotate": "0.3.2",
    "babel-plugin-syntax-decorators": "6.13.0",
    "babel-preset-angular": "6.0.15",
    "babel-preset-es2015": "6.24.1",
    "bootstrap": "3.3.7",
    "bootswatch": "3.3.7",
    "font-awesome": "4.7.0",
    "jquery": "3.3.1",
    "ng-tags-input": "3.0.0",
    "node-sass": "4.7.2",
    "postcss-modules": "1.1.0"
  },
  "devDependencies": {
    "parcel-bundler": "1.7.0",
    "rimraf": "2.6.2"
  }
}
