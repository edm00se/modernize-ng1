{
  "name": "modernize-ng1",
  "version": "1.0.0",
  "description": "a sample Angular 1.x app for getting set up with parcel bundler...\n\nnow adapted to use my app of Ice and Fire front-end, for a non-trivial example",
  "main": "src/main.js",
  "scripts": {
    "clean": "rimraf dist",
    "dev": "parcel ./src/index.html",
    "format": "prettier --write \"src/**/*.{js,json}\"",
    "build": "parcel build ./src/index.html --public-url .",
    "build:sw": "rimraf dist/sw.js && workbox generateSW config/workbox-config.js",
    "build:travis": "parcel build ./src/index.html --public-url . --log-level 2",
    "deploy": "npm run build && gh-pages -d dist",
    "test": "jest --config config/tests/jest.conf.js",
    "test:watch": "npm test -- --watch"
  },
  "keywords": [],
  "author": "Eric McCormick (https://edm00se.codes/)",
  "license": "MIT",
  "dependencies": {
    "@uirouter/angularjs": "1.0.20",
    "angular": "1.7.5",
    "angular-aria": "1.7.5",
    "angular-messages": "1.7.5",
    "angular-sanitize": "1.7.5",
    "bootstrap": "3.3.7",
    "bootswatch": "3.3.7",
    "font-awesome": "4.7.0",
    "jquery": "3.3.1",
    "ng-tags-input": "3.2.0"
  },
  "devDependencies": {
    "angular-mocks": "1.7.5",
    "babel-core": "6.26.3",
    "babel-jest": "23.6.0",
    "babel-plugin-ng-annotate": "0.3.2",
    "babel-plugin-syntax-decorators": "6.13.0",
    "babel-preset-env": "1.7.0",
    "jest": "23.6.0",
    "node-sass": "4.9.4",
    "parcel-bundler": "1.10.3",
    "prettier": "1.15.3",
    "rimraf": "2.6.2",
    "workbox-cli": "3.6.3"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 10",
    "ie 11"
  ]
}
